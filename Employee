#include <stdio.h>
#include <stdlib.h>
 
typedef struct
{
 
    char name[10];
    int age;
    int pn;
    int sal;
 
} 
Employee;
 
int main()
{
    int i, n = 20;
 
    Employee employees[20];
    for(i = 0; i < n; i++)
    {
        printf("Enter name : ");
        scanf("%s",employees[i].name);
        printf("Enter age : ");
        scanf("%d",&employees[i].age);
        printf("Enter Phone Number : ");
        scanf("%d",&employees[i].pn);
        printf("Enter salary : ");
        scanf("%d",&employees[i].sal);
        printf("Name \t: ");
        printf("%s \n",employees[i].name);
    }
    for(int i = 0; i < n; i++)
    {
 
        printf("Age \t: ");
        printf("%d \n",employees[i].age);
 
        printf("Phone Number \t: ");
        printf("%d \n",employees[i].pn);
        
        printf("Salary \t: ");
        printf("%d \n",employees[i].sal);
 
        printf("\n");
    }
 
    return 0;
}
