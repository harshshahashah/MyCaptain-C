#include <stdio.h>

int main()
{
    int mat[3][3];
    int sum;
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            printf("Input element [%d][%d] :",i,j);
            scanf("%d",&mat[i][j]);
        }
    }
    printf("The matrix is :");
    for(int i = 0; i < 3; i++)
    {
        for(int j = 0; j < 3; j++)
        {
            printf("%d",mat[i][j]);
        }
    }
    sum = mat[0][0] + mat[1][1] + mat[2][2];
    printf("\nThe sum of diagonal elements is %d",sum);
}
